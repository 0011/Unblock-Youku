{
  "name": "Unblock Youku",
  "version": "1.4",
  "manifest_version": 2,
  "minimum_chrome_version": "18.0",
  "description": "A Chrome extension for browsing Youku/Tudou/Ku6/Xiami online videos/audios from overseas, without proxy!",
  "icons": { "16": "icons/unblock-youku-16.png",
             "48": "icons/unblock-youku-48.png",
            "128": "icons/unblock-youku-128.png"
  },
  "permissions": [
    "webRequest", "webRequestBlocking",
    "http://*.youku.com/*",
    "http://*.tudou.com/*",
    "http://tudou.letv.com/*",
    "http://*.tudou.letv.com/*",
    "http://*.xiami.com/*",
    "http://*.ku6.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
